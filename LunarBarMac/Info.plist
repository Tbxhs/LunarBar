<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>LSUIElement</key>
	<true/>
	<key>NSAppleEventsUsageDescription</key>
	<string>Grant LunarBar permission to reveal the selected date in the Calendar app.</string>
	<key>NSCalendarsUsageDescription</key>
	<string>Grant LunarBar permission to display Calendar events.</string>
	<key>NSCalendarsFullAccessUsageDescription</key>
	<string>Grant LunarBar permission to display Calendar events.</string>
	<key>NSRemindersUsageDescription</key>
	<string>Grant LunarBar permission to display items in the Reminders app.</string>
	<key>NSRemindersFullAccessUsageDescription</key>
	<string>Grant LunarBar permission to display items in the Reminders app.</string>

	<!-- Sparkle 2 configuration -->
	<key>SUFeedURL</key>
	<string>https://tbxhs.github.io/LunarBar/appcast.xml</string>
	<key>SUPublicEDKey</key>
	<string>REPLACE_WITH_YOUR_ED25519_PUBLIC_KEY</string>
	<key>SUEnableAutomaticChecks</key>
	<true/>
</dict>
</plist>
